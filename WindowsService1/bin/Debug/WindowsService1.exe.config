<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<behaviors>
		<endpointBehaviors>
			<behavior name="DisableSSLCertificateValidation">
				<clientCredentials>
					<serviceCertificate>
						<sslCertificateAuthentication certificateValidationMode="None" />
					</serviceCertificate>
				</clientCredentials>
			</behavior>
		</endpointBehaviors>
	</behaviors>
</configuration>